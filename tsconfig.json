{
    "compilerOptions": {
        "outDir": "./",
        "sourceMap": true,
        "noImplicitAny": true,
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "node",
        "types": [
            "reflect-metadata"
        ],
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "exclude": [
        "./node_modules/**/*"
    ]
}